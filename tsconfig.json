{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": [
      "node"
    ],
    "paths": {
      "~/*": [
        "./client/*"
      ],
      "@/*": [
        "./client/*"
      ]
    },
  },
  "exclude": [
    "nuxt2",
  ]
}
